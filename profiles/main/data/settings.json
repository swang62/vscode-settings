  "tailwindCSS.experimental.classRegex": [
    [
      "Style \\=([^;]*);",
      "'([^']*)'"
    ],
    [
      "Style \\=([^;]*);",
      "\"([^\"]*)\""
    ],
    [
      "Style \\=([^;]*);",
      "\\`([^\\`]*)\\`"
    ]
  ],
  "continue.pauseTabAutocompleteOnBattery": true,
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "addWords": true,
      "scope": "user"
