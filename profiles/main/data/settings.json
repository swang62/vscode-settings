{
  "ShortcutMenuBar.navigateBack": false,
  "ShortcutMenuBar.navigateForward": false,
  "ShortcutMenuBar.openFilesList": false,
  "ShortcutMenuBar.saveAll": true,
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[caddyfile]": {
    "editor.defaultFormatter": "matthewpi.caddyfile-support"
  },
  "[dart]": {
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": "off"
  },
  "[dockercompose]": {
    "editor.autoIndent": "advanced"
  },
  "[github-actions-workflow]": {
    "editor.autoIndent": "advanced"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[yaml]": {
    "editor.autoIndent": "advanced"
  },
  "autoDocstring.docstringFormat": "one-line-sphinx",
  "autoDocstring.quoteStyle": "'''",
  "black-formatter.args": [
    "--line-length=90"
  ],
  "caddyfile.executable": "C:\\ProgramData\\chocolatey\\bin\\caddy.exe",
  "continue.enableTabAutocomplete": true,
  "continue.showInlineTip": false,
  "continue.telemetryEnabled": false,
  "database-client.highlightSQLBlock": true,
  "debug.console.fontSize": 14,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoSurround": "quotes",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always",
    // "source.organizeImports": "always",
    // "source.sortImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.foldingMaximumRegions": 50000,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "selection",
  "editor.selectionHighlight": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "textMateRules": [
        {
          "scope": [
            "entity.name.function"
          ],
          "settings": {
            "foreground": "#dbd8aa"
          }
        },
        {
          "scope": [
            "variable.parameter",
            "variable.other",
            "variable.name"
          ],
          "settings": {
            "foreground": "#e6e6e6"
          }
        },
        {
          "scope": [
            "variable.other.readwrite",
            "variable.other.constant"
          ],
          "settings": {
            "foreground": "#70a5c9"
          }
        }
      ]
    }
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "toml",
    "xml",
    "gql",
    "graphql",
    "astro",
    "css",
    "less",
    "scss",
    "pcss",
    "postcss"
  ],
  "eslint.rules.customizations": [
    {
      "rule": "style/*",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-indent",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-spacing",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-spaces",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-order",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-dangle",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-newline",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*quotes",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*semi",
      "severity": "off",
      "fixable": true
    }
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoCheckUpdates": true,
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.css": "tailwindcss",
    ".gcloudignore": "ignore"
  },
  "files.autoSave": "off",
  "files.autoSaveWorkspaceFilesOnly": true,
  "files.eol": "\n",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "git.mergeEditor": false,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.advanced.fileHistoryShowAllBranches": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.codeLens.authors.command": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.command": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.format": "",
  "gitlens.currentLine.scrollable": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.menus": {
    "editor": false,
    "editorGroup": false,
    "editorTab": false,
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
